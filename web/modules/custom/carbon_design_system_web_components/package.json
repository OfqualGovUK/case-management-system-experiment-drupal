{
  "name": "carbon_design_system_web_components",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "build": "for dir in components/*/; do component=$(basename \"$dir\"); if [ -f \"$dir/src/$component.js\" ]; then vite build --mode $component; fi; done",
    "preview": "vite preview"
  },
  "dependencies": {
    "@carbon/icons": "^11.70.0",
    "@carbon/styles": "^1.95.0",
    "@carbon/web-components": "^2.43.0",
    "glob": "^10.0.0",
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "sass": "^1.94.2",
    "sass-embedded": "^1.93.3"
  }
}

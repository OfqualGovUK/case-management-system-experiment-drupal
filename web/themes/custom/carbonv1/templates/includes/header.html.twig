<cds-header aria-label="{{site_name | default('site')}}" class="cds--header">
  <cds-header-menu-button button-label-active="Close menu" button-label-inactive="Open menu"></cds-header-menu-button>
  <cds-header-name href="javascript:void 0" prefix="Ofqual">Cases</cds-header-name>
  <cds-header-nav menu-bar-label="Ofqual Cases" style="color: green;">
{#    <cds-header-nav-item href="{{ path('cases.page') }}">Cases</cds-header-nav-item>#}
    <cds-header-menu menu-label="Profiles" trigger-content="Profiles">
      <cds-header-menu-item href="javascript:void 0">Regulated organisations</cds-header-menu-item>
      <cds-header-menu-item href="javascript:void 0">Subject matter specialists</cds-header-menu-item>
      <cds-header-menu-item href="javascript:void 0">Prospective organisations</cds-header-menu-item>
    </cds-header-menu>
    {% if logged_in %}
      <cds-header-nav-item href="{{ path('user.logout') }}">Logout</cds-header-nav-item>
      <cds-header-nav-item href="{{ path('user.page') }}">My Account</cds-header-nav-item>
    {% else %}
      <cds-header-nav-item href="{{ path('user.login') }}">Login</cds-header-nav-item>
    {% endif %}
  </cds-header-nav>
  <cds-side-nav is-not-persistent="" aria-label="Side navigation" collapse-mode="responsive">
    <cds-side-nav-items>
      <cds-side-nav-link href="javascript:void(0)">
        Link 1
      </cds-side-nav-link>
      <cds-side-nav-link href="javascript:void(0)">
        Link 2
      </cds-side-nav-link>
      <cds-side-nav-link href="javascript:void(0)">
        Link 3
      </cds-side-nav-link>
      <cds-side-nav-menu title="Link 4">
        <cds-side-nav-menu-item href="https://www.carbondesignsystem.com/">
          Sub-link 1
        </cds-side-nav-menu-item>
        <cds-side-nav-menu-item href="https://www.carbondesignsystem.com/">
          Sub-link 2
        </cds-side-nav-menu-item>
        <cds-side-nav-menu-item href="https://www.carbondesignsystem.com/">
          Sub-link 3
        </cds-side-nav-menu-item>
      </cds-side-nav-menu>
    </cds-side-nav-items>
  </cds-side-nav>
</cds-header>

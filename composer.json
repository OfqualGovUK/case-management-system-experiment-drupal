{
  "name": "ofqualgovuk/case-management-system-experiment-drupal",
  "description": "PoC Project for Ofqual Case Management",
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "php": "^8.3.0",
    "composer/installers": "^2.3",
    "drupal/admin_toolbar": "^3.6",
    "drupal/coffee": "2.0.1",
    "drupal/core-composer-scaffold": "^11.2",
    "drupal/core-project-message": "^11.2",
    "drupal/core-recipe-unpack": "^11.2",
    "drupal/core-recommended": "^11.2",
    "drupal/externalauth": "^2.0",
    "drupal/key": "^1.20",
    "drupal/openid_connect": "^3.0.0-alpha6",
    "drupal/simple_oauth": "^6.0",
    "drupal/ui_patterns": "^2.0",
    "drush/drush": "^13.6"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "drupal/core-composer-scaffold": true,
      "drupal/core-recipe-unpack": true,
      "drupal/core-project-message": true,
      "phpstan/extension-installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "php-http/discovery": true,
      "php-tuf/composer-integration": true
    },
    "sort-packages": true,
    "platform": {
      "php": "8.3.0"
    },
    "preferred-install": "dist"
  },
  "extra": {
    "installer-paths": {
        "web/core": ["type:drupal-core"],
        "web/libraries/{$name}": ["type:drupal-library"],
        "web/modules/contrib/{$name}": ["type:drupal-module"],
        "web/profiles/contrib/{$name}": ["type:drupal-profile"],
        "web/themes/contrib/{$name}": ["type:drupal-theme"],
        "drush/contrib/{$name}": ["type:drupal-drush"],
        "type:drupal-custom-theme": ["type:drupal-custom-theme"]
    }
  },
  "require-dev": {
    "drupal/coder": "8.3",
    "squizlabs/php_codesniffer": "3.13"
  }
}
